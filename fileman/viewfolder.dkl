    printUp::path
    {
        path=replace(path,"\\","/")
        if not(contains(path,"/")) || path=="" || path=="/" { return }
        path=dir.name(path)
        
        ##
        div(class="back")
        {
            a(href="#<path.concat(path,@fileman_viewtrigger+'?view='+@view_style)>" class="up")
            {
                svg(xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor" class="bi bi-arrow-up" viewBox="0 0 16 16"){
