    printFilesCard::&files,base_path,uri
    {
        for i=0;i<@count(files)
        {
            ref file=@item(files,i)
            filename=@@(file,'name')

            ref obj_info=dklfso.createObjectInfo(base_path,path.concat(uri,filename))
            
            ##
			div(class="file_container_card")
			{
                div(class="controls_above")
                {
                    input(type="checkbox" onclick="fileman.checkboxFile(this,'#<filename>');")
                }
                
				a(href="#<@@(file,'fullpath')>" class="file_container_box_card a_fileman")
				{
                    ##
                    ext = file.extension(@@(file,'name'))
                    icon = @fileman_basepath+"/icons/.defualt.icon.png"
                    extIcon =path.concat(path.concat(dir.name(@@(@http_context,'request/headers/path_translated')),"icons"),ext+".icon.png")
                    
                    if file.exists(extIcon){ icon = path.concat(path.concat(@fileman_basepath,"icons"),ext+".icon.png")}

                    ##
                    div(class="section_card name_card")
                    {
                        img(class="icon_card default_icon_card" src="#<icon>")
					    span(class="filename" title="#<@@(file,'name')>"){$"#<@@(file,'name')>"}
                    }
                    div(class="section_card title_card")
                    {
                        #! title = @@(obj_info,"properties/web_meta_tags/title")
                        #$ span(title="#<title>"){strong{$"#<title>"}}
